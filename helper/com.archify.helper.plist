<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.archify.helper</string>
	<key>MachServices</key>
	<dict>
		<key>com.archify.helper</key>
		<true/>
	</dict>
	<key>Program</key>
	<string>/Library/PrivilegedHelperTools/com.archify.helper</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/PrivilegedHelperTools/com.archify.helper</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>SMAuthorizedClients</key>
	<array>
		<string>identifier "com.archify.app" and anchor apple generic and (certificate leaf[subject.CN] = "Limited" and certificate 1[field.1.2.840.113635.100.6.2.1] /* exists */ and certificate leaf[subject.OU] = "1234567890")</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/var/log/com.archify.helper.err</string>
	<key>StandardOutPath</key>
	<string>/var/log/com.archify.helper.out</string>
</dict>
</plist>
